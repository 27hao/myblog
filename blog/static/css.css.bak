
body{
	background-color:#f8f8f8;
	font-family: 'Ubuntu',"Helvetica Neue",Helvetica,Arial,sans-serif,"Microsoft YaHei";
}
#body{
	width:65%;
	height:100%;
	left:50%;
	margin-left: -33%;
	border-radius:30px 30px 30px 30px;
	position:absolute;
}
#head{
	width:100%;
	border-radius:30px 30px 0px 0px;
	-webkit-border-radius:30px 30px 0px 0px;
	-moz-border-radius:25px; /* Old Firefox */
	background: #417690;
	
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#417690', endColorstr='#ffffff',GradientType=1 ); 
}
#head div{
	background-color:white;
	height:40px;
	border-bottom:1px solid rgb(226, 226, 226);
	border-top:1px solid rgb(226, 226, 226);
	padding-left:20px;
}
#head h1{
	height:40px;
	padding:30px;
	color: white;
}

#head div a{
	float:left;
	width:10%;
	height:100%;
	text-align:center;
	padding-top:6px;
	
}
#head div a{
}
a{font-size:20px;color:gray}   
a:link {text-decoration:none;} 
a:hover { text-decoration:underline;color:#333333;} 
#center{
	background-color:white;
	width:100%;
}
#center ul{
	list-style:none;
}
#article{
	border-bottom:1px solid rgb(226, 226, 226);
	padding:25px;
}
#article a{
font-size:15px;
margin-right:10px;
text-decoration:none
}
#article #a{
	background-color:rgb(226, 226, 226);
}

#article p a{
	font-size:30px;
	background-color:white;
	text-decoration:none
}
#foot{
	width:100%;
	height:30px;
	border-radius:0px 0px 30px 30px;
	-moz-border-radius:25px; /* Old Firefox */
	-webkit-border-radius:0px 0px 30px 30px;
	background:#417690;
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#417690', endColorstr='#ffffff',GradientType=1 ); 
	position:absolute;
}
